	 <action name="flooralpha">
        set(layer[floor_1].visible,true);
		set(layer[floor_2].visible,true);
		set(layer[floor_3].visible,true);
		set(layer[floor_4].visible,true);		
		<!-- 如需增加楼层 在这里添加-->

		set(layer[floor_1].alpha,0.4);
		set(layer[floor_2].alpha,0.4);
		set(layer[floor_3].alpha,0.4);
		set(layer[floor_4].alpha,0.4);
		<!-- 如需增加楼层 在这里添加-->
		set(layer[%1].alpha,1);

		set(layer[floor_1].scale,1);
		set(layer[floor_2].scale,1);
		set(layer[floor_3].scale,1);
		set(layer[floor_4].scale,1);
		<!-- 如需增加楼层 在这里添加-->
		set(layer[%1].scale,1.2);
	</action>
	
	<action name="floorurl">		
		set(layer[floor_1].onclick,loadscene(%1,null,MERGE,BLEND(0.5)));
		set(layer[floor_2].onclick,loadscene(%2,null,MERGE,BLEND(0.5)));
		set(layer[floor_3].onclick,loadscene(%3,null,MERGE,BLEND(0.5)));
		set(layer[floor_4].onclick,loadscene(%4,null,MERGE,BLEND(0.5)));	
		<!-- 如需增加楼层 在这里添加-->
	</action>
	
	<action name="floorvisible">
		set(layer[floor_1].visible,false);
		set(layer[floor_2].visible,false);
		set(layer[floor_3].visible,false);
		set(layer[floor_4].visible,false);		
		<!-- 如需增加楼层 在这里添加-->
	</action>
	
	<action name="activemap">
	  set(layer[map_container].visible, true);
      set(layer[map_1].visible, false);
	  set(layer[map_2].visible, false);
	  set(layer[map_3].visible, false);
	  set(layer[map_4].visible, false);
		
	    <!-- 如需增加楼层 在这里添加-->
		set(layer[%1].visible, true);
		set(layer[map].url, %2);
		set(layer[map].scale, %3);	
	</action>
	
	
	
	<!-- 以下是核心参数不能做任何修改 建议用户自己在编辑个XML加密保存 -->
	<action name="activatespot">
		txtadd(spot_index,'spot',get(scene[get(xml.scene)].index));
		layer[map_1].setcenter(0,0);
		copy(layer[active_spot].x, layer[get(spot_index)].x);
		copy(layer[active_spot].y, layer[get(spot_index)].y);
		copy(layer[radar].x, layer[get(spot_index)].x);
		copy(layer[radar].y, layer[get(spot_index)].y);
		set(layer[radar].heading, %1);
		set(layer[radar].visible, true);
	</action>
	
    <action name="addhotspotthumb">
       addlayer(addhotspotthumb);
		   set(layer[addhotspotthumb].align, lefttop);
		   set(layer[addhotspotthumb].keep, true);
		   copy(layer[addhotspotthumb].x, mouse.stagex);
		   copy(layer[addhotspotthumb].y, mouse.stagey);
		   set(layer[addhotspotthumb].ox, -240);
		   set(layer[addhotspotthumb].oy, 10);
		   set(layer[addhotspotthumb].width, 240);
		   set(layer[addhotspotthumb].height, 180);
		   subtxt(scene_indexnum, %1, 4, 2);
		   copy(layer[addhotspotthumb].url,scene[get(scene_indexnum)].thumburl);
		   set(layer[addhotspotthumb].zorder, 92);
		   set(layer[addhotspotthumb].effect,glow(0xFFFFFF,0.9,10,10));
		   addlayer(hotspotthumbtext);
       layer[hotspotthumbtext].loadstyle(skin_thumbtext_style1);
	     set(layer[hotspotthumbtext].keep, true);
       set(layer[hotspotthumbtext].parent, addhotspotthumb);
       set(layer[hotspotthumbtext].html, get(scene[get(scene_indexnum)].title));   
		</action>
		
		<action name="loadscenehotspot">       
		   subtxt(scene_indexnum_1, %1, 4, 2);
		   loadscene(get(scene[get(scene_indexnum_1)].name),null,MERGE,BLEND(0.5));
		</action>
	
    <action name="removehotspotthumb">	
       removelayer(addhotspotthumb);	 
		   removelayer(hotspotthumbtext);
	    </action>

